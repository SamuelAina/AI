<script type="text/javascript">
    function setHoverByTD3_Content(td3_content, hover_text) {
        var serach_str = "td:contains('" + td3_content + ":')";
        var content_str = "<td><a href='#' class='tip'>" + td3_content + "<span>" + hover_text + "</span></a>:</td>"
        $(serach_str).eq(3).html(content_str);
    }

    var result = $("#usp_hover_context").val();
    $.each(
            JSON.parse(result),
            function (index, value) {
                console.log(value.field_name);
                setHoverByTD3_Content(value.field_name, value.hover_text);
            }
        );
     
</script>

<script type="text/javascript">
    function applyCommentCollapseBehaviour(field_name) {
        field_name = field_name.replace(/\s/g, '');
        var fld_pattern = "#" + field_name;
        var fld_option_selected_pattern = "#" + field_name + " option:selected"
        var fld_row_pattern = "#" + field_name + "Comment_row";
        $(fld_pattern).bind("click", function () {
            if ($(fld_option_selected_pattern).text() == "No") {
                $(fld_row_pattern).show()
            } else {
                $(fld_row_pattern).hide()
            }
        });
        $(fld_pattern).trigger("click");
    }

    //Apply Comment Collapse to array of fields from the database
    $.each(
        $("#usp_CommentCollapse_fields").val().split('|'),
        function (index, value) { applyCommentCollapseBehaviour(value); }
        );

		
</script>

<script type="text/javascript">
  //Populate upper error message span
	$("#msg2")[0].innerText=  $("#msg")[0].innerText;
  
  //CApproval, GFDFRApproval, FR/MR locking rules
    var  CCApproval, GFDFRApproval, FR_MR;
	if ($("#CCApproval").length) {
		CCApproval = $($("#CCApproval:options")[0]).children("option:selected").val()
	}
	if ($("#CCApproval_static").length) {
		CCApproval = $("#CCApproval_static").text()
	}

	if ($("#GFDFRApproval").length) {
		GFDFRApproval = $($("#GFDFRApproval:options")[0]).children("option:selected").val();
	}
	if ($("#GFDFRApproval_static").length) {
		GFDFRApproval = $("#GFDFRApproval_static").text();
	} 
	 
	if ($("#GFDFRApproval").length) {
		GFDFRApproval = $($("#GFDFRApproval:options")[0]).children("option:selected").val();
	}
	
	if ($("select[id*=FR/MR]").length) {
		FR_MR = $($('select[id*=FR/MR]:options')[0]).children("option:selected").val()
	}
	if ($("[id*=FR/MR_static]").length) {
		FR_MR = $("[id*=FR/MR_static]").text();
	}	
	
    //Lock fields based on CCM Approval/Rejection -- Affected fields: GFD MR Approval, GFD FR Approval
	$("#GFDFRApprovallockedtilcca").empty();
	if (!(["Approved", "Rejected"].includes(CCApproval))) {
		$("#GFDFRApproval").attr('disabled', true)
		$("#GFDFRApproval").after("<font id=GFDFRApprovallockedtilcca  color=red>.locked until CC Approval/Rejection</font>")
	} else {
		$("#GFDFRApproval").attr('disabled', false)
	}

	$("#GFDMRApprovallockedtilcca").empty();
	if (!(["Approved", "Rejected"].includes(CCApproval))) {
		$("#GFDMRApproval").attr('disabled', true)
		$("#GFDMRApproval").after("<font id=GFDMRApprovallockedtilcca  color=red>.locked until CC Approval/Rejection</font>")
	} else {
		$("#GFDMRApproval").attr('disabled', false)
	}

    //Lock fields based on GFD FR Approval Approval/Rejection -- affected fields: GFD MR Approval 
	//Include FR/MR exception
	//If “MR” is selected from “FR/MR” field, tracker should skip Group FR approval and should only go for Group MR approval
	$("#GFDMRApprovallockedtilFR").empty();
	if (!(["Approved", "Rejected"].includes(GFDFRApproval))) {
		if(!(FR_MR=="MR")){
		     $("#GFDMRApproval").attr('disabled', true)
		     $("#GFDMRApproval").after("<font id=GFDMRApprovallockedtilFR  color=purple>.locked until FR Approval/Rejection</font>")
         }		
	} else {
		$("#GFDMRApproval").attr('disabled', false)
	}
</script>
<script type="text/javascript">
    //Conditional locking of fields for Requestor if CFO Approval="Approved")
	$( document ).ready(function() {
		JSON.parse($("#usp_Requestor_locked_fields_when_cfo_approved_json").val().replace(/``/g, '"'))
		.forEach(function (i) {
			$("[name=" + i.field_name + "]").attr("disabled", i.locked);
		})
	});	

</script>


