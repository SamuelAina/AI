DECLARE @text NVARCHAR(100) = '12246ABC'
DECLARE @result NVARCHAR(100) = ''

WHILE LEN(@text) > 0
BEGIN
    IF PATINDEX('%[0-9]%', @text) = 1
    BEGIN
        SET @result = @result + SUBSTRING(@text, 1, 1)
    END
    SET @text = SUBSTRING(@text, 2, LEN(@text) - 1)
END

SELECT @result AS NumericCharacters
