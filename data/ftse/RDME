CREATE FUNCTION dbo.GetNumericChars (@input NVARCHAR(MAX))
RETURNS NVARCHAR(MAX)
AS
BEGIN
    DECLARE @output NVARCHAR(MAX) = ''
    DECLARE @len INT = LEN(@input)
    DECLARE @i INT = 1
    DECLARE @char NCHAR(1)

    WHILE @i <= @len
    BEGIN
        SET @char = SUBSTRING(@input, @i, 1)
        IF @char LIKE '[0-9]'
        BEGIN
            SET @output = @output + @char
        END
        SET @i = @i + 1
    END

    RETURN @output
END
GO
