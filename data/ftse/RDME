<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LockData Checkboxes</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }
        .checkbox-container {
            display: flex;
            flex-direction: column;
            gap: 10px;
            padding: 20px;
            max-width: 300px;
            margin: 0 auto;
        }
        label {
            display: flex;
            align-items: center;
            gap: 10px;
        }
        input[type="checkbox"] {
            transform: scale(1.2);
        }
    </style>
</head>
<body>
    <div id="checkbox-container" class="checkbox-container"></div>

    <script>
        const LockData = [
            {"period":"HY 2022","locked":0},
            {"period":"FY 2022","locked":0},
            {"period":"HY 2023","locked":0},
            {"period":"FY 2023","locked":0},
            {"period":"HY 2024","locked":0},
            {"period":"FY 2024","locked":0}
        ];

        const container = document.getElementById('checkbox-container');

        LockData.forEach((data, index) => {
            const label = document.createElement('label');
            const checkbox = document.createElement('input');
            checkbox.type = 'checkbox';
            checkbox.checked = data.locked === 1;
            checkbox.id = data.period;

            // Update LockData when checkbox is clicked
            checkbox.addEventListener('change', () => {
                LockData[index].locked = checkbox.checked ? 1 : 0;
                console.log(LockData);
            });

            label.appendChild(checkbox);
            label.appendChild(document.createTextNode(data.period));

            container.appendChild(label);
        });
    </script>
</body>
</html>


SELECT  
DISTINCT 
bg_reported_date,
   CASE 
        WHEN bg_reported_date BETWEEN CONVERT (DATETIME,'1 Jan '+ CONVERT(VARCHAR(10),YEAR(bg_reported_date))) AND CONVERT (DATETIME,'30 Jun '+ CONVERT(VARCHAR(10),YEAR(bg_reported_date))) THEN 'HY '+CONVERT(VARCHAR(10),YEAR(bg_reported_date))
        WHEN bg_reported_date BETWEEN CONVERT (DATETIME,'1 Jul '+ CONVERT(VARCHAR(10),YEAR(bg_reported_date))) AND CONVERT (DATETIME,'31 Dec '+ CONVERT(VARCHAR(10),YEAR(bg_reported_date))) THEN 'FY '+CONVERT(VARCHAR(10),YEAR(bg_reported_date))
   END
FROM bugs




SELECT 
    bg_reported_date,
    CASE 
        WHEN bg_reported_date BETWEEN DATEFROMPARTS(YEAR(bg_reported_date), 1, 1) AND DATEFROMPARTS(YEAR(bg_reported_date), 6, 30) THEN 'HY ' + CONVERT(VARCHAR(10), YEAR(bg_reported_date))
        WHEN bg_reported_date BETWEEN DATEFROMPARTS(YEAR(bg_reported_date), 7, 1) AND DATEFROMPARTS(YEAR(bg_reported_date), 12, 31) THEN 'FY ' + CONVERT(VARCHAR(10), YEAR(bg_reported_date))
    END AS period
FROM 
    bugs
GROUP BY 
    bg_reported_date,
    CASE 
        WHEN bg_reported_date BETWEEN DATEFROMPARTS(YEAR(bg_reported_date), 1, 1) AND DATEFROMPARTS(YEAR(bg_reported_date), 6, 30) THEN 'HY ' + CONVERT(VARCHAR(10), YEAR(bg_reported_date))
        WHEN bg_reported_date BETWEEN DATEFROMPARTS(YEAR(bg_reported_date), 7, 1) AND DATEFROMPARTS(YEAR(bg_reported_date), 12, 31) THEN 'FY ' + CONVERT(VARCHAR(10), YEAR(bg_reported_date))
    END;


